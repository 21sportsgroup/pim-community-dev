<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="pim_icecat_connector.connectorservice.class">Pim\Bundle\ConnectorIcecatBundle\Service\ConnectorService</parameter>
    </parameters>

    <services>
        <service id="akeneo.connector.icecat_service" class="%pim_icecat_connector.connectorservice.class%">
            <argument type="service" id="service_container" />
            <tag name="akeneo.connector" />
        </service>
    </services>

</container>
