{
    "name": "akeneo/TranslationBundle",
    "type": "symfony-bundle",
    "description": "PIM Translation Bundle",
    "keywords": ["PIM", "I18n", "Translation"],
    "homepage": "https://github.com/akeneo/TranslationBundle",
    "license": "OSL",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.6"
    },
    "require-dev": {
        "doctrine/dbal": ">=2.2,<2.4-dev",
        "doctrine/orm": ">=2.2.3,<2.4-dev"
    },
    "autoload": {
        "psr-0": { "Pim\\Bundle\\TranslationBundle": "" }
    },
    "target-dir": "Pim/Bundle/TranslationBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}